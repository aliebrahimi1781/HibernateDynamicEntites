<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">

    <class abstract="false" name="com.motech.test.persistEntityDefinition.TableDefinition" table="tbl_table_definition">

        <id column="fld_id" name="id">
            <generator class="native"/>
        </id>

        <property name="tableName" column="fld_table_name" type="string"/>
        <property name="tableDefinition" column="fld_table_definition" type="string"/>


    </class>
</hibernate-mapping>